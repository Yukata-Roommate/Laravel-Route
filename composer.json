{
    "name": "yukata-roommate/laravel-route",
    "description": "Easy to define routes in Laravel",
    "version": "1.0.0",
    "keywords": [
        "yukata-roommate",
        "php",
        "laravel",
        "package",
        "route",
        "laravel-route"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Yukata Roommate"
        }
    ],
    "autoload": {
        "psr-4": {
